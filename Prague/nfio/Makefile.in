top_builddir=@top_builddir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
VPATH=@srcdir@

Name = nfio
Plugin = $(Name).@PLUGIN_EXT@
Metafile = @srcdir@/$(Name).@METAFILE_EXT@
Targets = $(Plugin)

Sources = \
	plugin_win32_nfio.c \
	util.c \
	win32_io.cpp \
	win32_objptr.c \
	win32_os.c

# unix section
UnixLibraries = \
	-lcompat

VersionScript = @abs_top_srcdir@/unix/prague_plugin.ver

include @abs_top_builddir@/@OS@/Rules
