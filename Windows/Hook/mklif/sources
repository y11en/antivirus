TARGETNAME=klif

!IFNDEF TARGETPATH
!IF "$(W2K)" == "_w2k"
TARGETPATH=..\..\..\out\mklif\$(DDKBUILDENV)_w2k
!ELSE
TARGETPATH=..\..\..\out\mklif\$(DDKBUILDENV)
!ENDIF
!ENDIF

PDBPATH=$(TARGETPATH)
TARGETTYPE=DRIVER
DRIVERTYPE=FS
LINKER_FLAGS=/NOLOGO

TARGETLIBS= $(TARGETLIBS) \
            $(IFSKIT_LIB_PATH)\fltMgr.lib

!IF "$(_BUILDARCH)"=="AMD64"
TARGETLIBS= $(TARGETLIBS) \
	    obj$(BUILD_ALT_DIR)\osspec_asm.obj
!ENDIF

INCLUDES=..\..\..\CommonFiles

