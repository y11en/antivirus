	HkCheckLock.c \
	klif.rc

!IF "$(W2K)" == "_w2k"
RUN_WPP=$(SOURCES) -km -gen:{km-w2k.tpl}*.tmh -func:DoTraceEx(LEVEL,FLAGS,MSG,...)
!ELSE
RUN_WPP= $(SOURCES) -km -func:DoTraceEx(LEVEL,FLAGS,MSG,...)
!ENDIF


